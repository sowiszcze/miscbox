{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "template": "\uf120 {{ .Name }} "
        },
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#047e84",
          "template": " {{.Icon}}{{ if .WSL }} @ \ue62a{{ end }} "
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#9A348E",
          "template": " {{ if .SSHSession }}\ueba9{{ end }}{{ if .Root }}\uf0e7{{ end }}{{ if or .SSHSession .Root }} {{ end }}{{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#DA627D",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\uf7db",
            "style": "agnoster"
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#000000",
          "background": "#00ffff",
          "template": " {{ if .Unsupported }}\uf071{{ else }}\ue77f {{ .Full }}{{ end }} "
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#FCA17D",
          "template": " âžœ ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}) ",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#86BBD8",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_package_manager": true
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#33658A",
          "template": " {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "2006-01-02 15:04:05 MST"
          }
        },
        {
          "type": "status",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#2e812e",
          "background_templates": [
            "{{ if .Error }}#812e2e{{ end }}"
          ],
          "template": " {{ if .Error }}\ufb8a {{ .Meaning }}{{ else }}\uf004{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ]
}
